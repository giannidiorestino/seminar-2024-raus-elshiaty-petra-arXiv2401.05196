@InProceedings{savarino|albers|schnoerr:2021,
  author =	 "Savarino, Fabrizio and Albers, Peter and
                  Schn{\"o}rr, Christoph",
  editor =	 "Elmoataz, Abderrahim and Fadili, Jalal and
                  Qu{\'e}au, Yvain and Rabin, Julien and Simon,
                  Lo{\"i}c",
  title =	 "On the Geometric Mechanics of Assignment Flows for
                  Metric Data Labeling",
  booktitle =	 "Scale Space and Variational Methods in Computer
                  Vision",
  year =	 2021,
  publisher =	 "Springer International Publishing",
  address =	 "Cham",
  pages =	 "398--410",
  abstract =	 "Assignment flows are a general class of dynamical
                  models for context dependent data classification on
                  graphs. These flows evolve on the product manifold
                  of probability simplices, called assignment
                  manifold, and are governed by a system of coupled
                  replicator equations. In this paper, we adopt the
                  general viewpoint of Lagrangian mechanics on
                  manifolds and show that assignment flows satisfy the
                  Euler-Lagrange equations associated with an action
                  functional. Besides providing a novel interpretation
                  of assignment flows, our result rectifies the
                  analogous statement of a recent paper devoted to
                  uncoupled replicator equations evolving on a single
                  simplex, and generalizes it to coupled replicator
                  equations and assignment flows.",
  isbn =	 "978-3-030-75549-2"
}

