@Article{	  ohara|amari:94,
  ams		= {93B29, 53C21, 93C05},
  author	= {Ohara, Atsumi and Amari, Shu{n-i}chi},
  journal	= {Kybernetika},
  mathrev	= {In this paper the theory of dual connections (which has
		  been extensively studied by Amari [Differential-geometrical
		  methods in statistics, Lecture Notes in Statist., 28,
		  Springer, New York, 1985; MR 86m:62053]) is used to
		  describe differential geometric structures of feedback
		  systems. Consider an $n$-dimensional linear system with $m$
		  inputs represented by the state space equation $
		  \dot{x}=Ax+Bu,$ where $(A,B)$ is stabilizable and $B$ is of
		  full column rank. Denote by ${\rm PD}(n),$ ${\rm Skew}(n)$
		  and $S(n),$ respectively, the sets of positive definite,
		  skew-symmetric and stable real $n \times n$ matrices. The
		  authors investigate the structure of the sets ${\scr F}\sb
		  S(A,B)$ and $S\sb f(A,B) \subseteq S(n)$ of stabilizing
		  state feedback gains and state feedback system matrices. It
		  is proved that $S(n)$ is diffeomorphic to a vector bundle
		  ${\rm PD}(n) \times {\rm Skew}(n),$ and both the base
		  manifold ${\rm PD}(n)$ and the bundle ${\rm PD}(n) \times
		  {\rm Skew}(n)$ are further endowed with Riemannian
		  structures. Moreover, $S\sb f(A,B)$ is diffeomorphic to an
		  (explicitly constructed) embedded submanifold of ${\rm
		  PD}(n) \times {\rm Skew}(n),$ and the set ${\scr F}\sb
		  S(A,B)$ is parametrized by the vector subbundle ${\rm PD}
		  (n;A,B,Q) \times {\rm Skew}(n;B)$ of ${\rm PD}(n) \times
		  {\rm Skew}(n)$ (${\rm PD}(n;A,B,Q)$ is a certain totally
		  geodesic submanifold of ${\rm PD}(n)$ and ${\rm Skew}(n;B)$
		  is a vector subspace of ${\rm Skew}(n)$). A Riemannian
		  metric $g$ on ${\rm PD}(n)$ invariant under basis
		  transformations of the state space is constructed, endowing
		  ${\rm PD}(n)$ with the structure of a Riemannian global
		  symmetric space. A similar construction yields an invariant
		  fibre metric $f$ on ${\rm PD} (n) \times {\rm Skew}(n)$. It
		  is well known that parallel displacement along a curve $c$
		  defines a connection. The authors use pairs of parallel
		  displacements $\Pi\sb c$ and $\Pi\sb c\sp {\ast}$ in ${\rm
		  PD}(n)$ and $\tilde{\Pi}\sb c$ and $\tilde{\Pi}\sb c\sp
		  {\ast}$ in ${\rm PD}(n) \times {\rm Skew}(n)$ satisfying
		  $g(X,Y)=g(\Pi\sb cX,\Pi\sb c\sp {\ast}Y)$ and
		  $f(X,Y)=f(\tilde{\Pi}\sb cX,\tilde{\Pi}\sb c\sp {\ast}Y)$
		  to obtain so-called dual connections $\nabla$ and
		  $\nabla\sp {\ast}$ in ${\rm PD}(n)$ and $\tilde{\nabla}$
		  and $\tilde{\nabla}\sp {\ast}$ in ${\rm PD}(n) \times {\rm
		  Skew}(n)$ (both $\frac12(\nabla+\nabla\sp {\ast})$ and
		  $\frac12(\tilde{\nabla}+\tilde{\nabla}\sp {\ast})$ are
		  metric connections; the former is the canonical
		  (Levi-Civita) connection in ${\rm PD}(n)$). With
		  connections thus defined, the Riemannian manifold ${\rm
		  PD}(n)$ and the vector bundle ${\rm PD}(n) \times {\rm
		  Skew}(n)$ turn out to have vanishing Riemann-Christoffel
		  tensors (i.e., are flat). The induced Riemannian metric and
		  induced dual connections define a flat structure on the
		  subbundle ${\rm PD}(n;A,B,Q) \times {\rm Skew}(n;B) \cong
		  {\scr F}\sb S(A,B)$. Finally, the submanifold ${\rm PD}
		  (n;A,B,Q) \times {\rm Skew}(n;B)$ embedded in a particular
		  way into ${\rm PD}(n) \times {\rm Skew}(n)$ (so that it embodies the structure of $S\sb f(A,B)$ in $S(n)$) is shown to be of nontrivial curvature},
  number	= {4},
  pages		= {369--386},
  title		= {Differential geometric structures of stable state feedback
		  systems with dual connections},
  volume	= {30},
  year		= {1994}
}

