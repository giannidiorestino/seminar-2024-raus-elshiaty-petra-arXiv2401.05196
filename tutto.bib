@InProceedings{	  amari:87,
  ams		= {62B10, 62G99},
  author	= {Amari, Shu{n-i}chi},
  booktitle	= {Proceedings of the 46th Session of the International
		  Statistical Institute, Vol. 2 (Tokyo, 1987)},
  mathrev	= {A statistical model is regarded as a submanifold imbedded
		  in the function space of all the probability distributions.
		  It is shown that a model has a natural differential
		  geometrical structure represented by the Riemannian metric
		  corresponding to the Fisher information and the dually
		  coupled connections. The present paper demonstrates the
		  geometrical structure of a statistical model and a new
		  differential geometrical method of statistical inference
		  based on it. It also explains how the geometrical structure
		  is related to the characteristics of statistical
		  inferential procedures. We also show various new results on
		  statistical problems which are derived by the geometrical
		  method. It is useful not only in a classical model but also
		  in the semiparametric or nonparametric models. Moreover, it
		  can be applied to general information sciences including
		  information theory and control systems theory},
  numper	= {2},
  pages		= {321--338},
  series	= {Bulletin de l'Institut International de Statistique},
  title		= {Differential geometry in statistical inference},
  volume	= {52},
  year		= {1987}
}

@InProceedings{	  amari:87dual,
  ams		= {62F12, 58B99},
  author	= {Amari, Shu{n-i}chi},
  booktitle	= {Geometrization of statistical theory (Lancaster, 1987)},
  location	= {Lancaster},
  mathrev	= {A fibre bundle is constructed on a finite-dimensional
		  parametric statistical model with a Hilbert space as the
		  fibre space. The Hilbert space represents the tangent
		  directions of the set of probability distributions in the
		  function space. A pair of dual linear connections is
		  introduced in the Hilbert bundle. This framework is used to
		  study the following problems. Let $x\sb i$ have the
		  probability distribution $p(x, \theta, \xi\sb i)$, $i=1,2,
		  \cdots,$ where $\theta$ is the parameter of main interest
		  and $\xi\sb 1, \xi\sb 2, \cdots $ are unknown nuisance
		  parameters which increase with $n$. The estimation of
		  $\theta$ through an estimating equation $\sum y(x\sb i,
		  \theta)=0$ is considered using the criterion of minimum
		  asymptotic variance for any sequence of nuisance
		  parameters. The same problem is considered under the
		  assumption that $\xi\sb 1, \xi\sb 2, \cdots $ are
		  independent random variables having a common distribution
		  $Z(\xi)$ which is considered as the nuisance parameter.
		  Such a model is called a semiparametric model},
  pages		= {123--151},
  publisher	= {ULDM Publ.},
  title		= {Dual connections on the {H}ilbert bundles of statistical
		  models},
  year		= {1987}
}

