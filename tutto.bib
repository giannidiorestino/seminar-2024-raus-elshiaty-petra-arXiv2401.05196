@Article{montrucchio|pistone:2021,
  author =	 {Montrucchio, Luigi and Pistone, Giovanni},
  title =	 {Kantorovich distance on finite metric spaces:
                  Arens--Eells norm and CUT norms},
  journal =	 {Information Geometry},
  year =	 {2021},
  month =	 {Jul},
  day =		 {01},
  abstract =	 {We study the possible closed-form representations of
                  the K-distance arising in the Kantorovich transport
                  problems on finite metric spaces. Weighted graphs,
                  {\$}{\$}{\backslash}ell {\_}1{\$}{\$}-embeddable
                  metrics, and the related
                  {\$}{\$}{\{}{\{}{\backslash},{\backslash}mathrm{\{}{\backslash}text
                  {\{}{\{}
                  CUT{\}}{\}}{\}}{\backslash},{\}}{\}}{\$}{\$}norms
