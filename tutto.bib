@Article{	  mertikopoulos2018315,
  title		= "Riemannian game dynamics",
  journal	= "Journal of Economic Theory",
  volume	= "177",
  pages		= "315 - 364",
  year		= "2018",
  issn		= "0022-0531",
  doi		= "https://doi.org/10.1016/j.jet.2018.06.002",
  url		= "http://www.sciencedirect.com/science/article/pii/S0022053118302679",
  author	= "Panayotis Mertikopoulos and William H. Sandholm",
  keywords	= "Evolutionary game theory, Learning in games, Projection
		  dynamics, Riemannian metrics, Replicator dynamics,
		  Reinforcement learning",
  abstract	= "We study a class of evolutionary game dynamics defined by
		  balancing a gain determined by the game's payoffs against a
		  cost of motion that captures the difficulty with which the
		  population moves between states. Costs of motion are
		  represented by a Riemannian metric, i.e., a state-dependent
		  inner product on the set of population states. The
		  replicator dynamics and the (Euclidean) projection dynamics
		  are the archetypal examples of the class we study. Like
		  these representative dynamics, all Riemannian game dynamics
		  satisfy certain basic desiderata, including positive
		  correlation, local stability of interior ESSs, and global
		  convergence in potential games. When the underlying
		  Riemannian metric satisfies a Hessian integrability
		  condition, the resulting dynamics preserve many further
		  properties of the replicator and projection dynamics. We
		  examine the close connections between Hessian game dynamics
		  and reinforcement learning in normal form games, extending
		  and elucidating a well-known link between the replicator
		  dynamics and exponential reinforcement learning."
}

